package com.capgemini.springcore.beans;

public interface Author {

	private String name;

	public Author(String name) {
		super();
		this.name = name;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public void makeSound() {
		System.out.println("eeeeeeeeeee");
	}
	
	
}

package com.cpg.empspringmvc.bean;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;

@Data
@Entity("EmployeeBean")
public class EmployeeBean {
	@Id
	@Column
	private int id;
	@Column
	private String name;
	@Column
	private String email;
	@Column
	private String password;

}
